    <connection>conn_perdido</connection>
    <sql>WITH RFC as &#x28;&#xa;				SELECT FECHA&#xa;				FROM CALIDAD_STATUS_REFERENCES&#xa;				WHERE FECHA BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;                                AND HORA &#x3d; &#x27;00&#x27;&#xa;			&#x29;,&#xa;	AVL as	&#x28;&#xa;				SELECT FECHA,&#xa;				   MAX&#x28;AVL_LBS&#x29; AVL_LBS,&#xa;				   MAX&#x28;AVL_ALM&#x29; AVL_ALM,&#xa;				   MAX&#x28;AVL_MRC&#x29; AVL_MRC,&#xa;				   MAX&#x28;AVL_CDD&#x29; AVL_CDD,&#xa;				   MAX&#x28;AVL_CTY&#x29; AVL_CTY&#xa;				FROM &#x28;&#xa;					SELECT FECHA,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;LNBTS&#x27;  , CANTIDAD&#x29; AVL_LBS,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;ALM&#x27;    , CANTIDAD&#x29; AVL_ALM,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;MERCADO&#x27;, CANTIDAD&#x29; AVL_MRC,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;CIUDAD&#x27; , CANTIDAD&#x29; AVL_CDD,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;PAIS&#x27;   , CANTIDAD&#x29; AVL_CTY&#xa;					FROM &#x28;&#xa;							SELECT FECHA, ELEMENT_CLASS, COUNT&#x28;&#x2a;&#x29; CANTIDAD&#xa;							FROM LTE_NSN_AVAIL_NE_DAY&#xa;							WHERE FECHA BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;							GROUP BY FECHA, ELEMENT_CLASS&#xa;						&#x29;&#xa;					&#x29;&#xa;				GROUP BY FECHA&#xa;			&#x29;,&#xa;	PAQ as  &#x28;&#xa;				SELECT FECHA,&#xa;				   MAX&#x28;PAQ_LBS&#x29; PAQ_LBS,&#xa;				   MAX&#x28;PAQ_ALM&#x29; PAQ_ALM,&#xa;				   MAX&#x28;PAQ_MRC&#x29; PAQ_MRC,&#xa;				   MAX&#x28;PAQ_CDD&#x29; PAQ_CDD,&#xa;				   MAX&#x28;PAQ_CTY&#x29; PAQ_CTY&#xa;				FROM &#x28;&#xa;					SELECT FECHA,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;LNBTS&#x27;  , CANTIDAD&#x29; PAQ_LBS,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;ALM&#x27;    , CANTIDAD&#x29; PAQ_ALM,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;MERCADO&#x27;, CANTIDAD&#x29; PAQ_MRC,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;CIUDAD&#x27; , CANTIDAD&#x29; PAQ_CDD,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;PAIS&#x27;   , CANTIDAD&#x29; PAQ_CTY&#xa;					FROM &#x28;&#xa;							SELECT FECHA, ELEMENT_CLASS, COUNT&#x28;&#x2a;&#x29; CANTIDAD&#xa;							FROM LTE_NSN_PAQ_NE_DAY&#xa;							WHERE FECHA BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;							GROUP BY FECHA, ELEMENT_CLASS&#xa;						&#x29;&#xa;					&#x29;&#xa;				GROUP BY FECHA&#xa;			&#x29;,&#xa;	SRV as &#x28;&#xa;				SELECT FECHA,&#xa;				   MAX&#x28;SRV_LBS&#x29; SRV_LBS,&#xa;				   MAX&#x28;SRV_ALM&#x29; SRV_ALM,&#xa;				   MAX&#x28;SRV_MRC&#x29; SRV_MRC,&#xa;				   MAX&#x28;SRV_CDD&#x29; SRV_CDD,&#xa;				   MAX&#x28;SRV_CTY&#x29; SRV_CTY&#xa;				FROM &#x28;&#xa;					SELECT FECHA,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;LNBTS&#x27;  , CANTIDAD&#x29; SRV_LBS,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;ALM&#x27;    , CANTIDAD&#x29; SRV_ALM,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;MERCADO&#x27;, CANTIDAD&#x29; SRV_MRC,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;CIUDAD&#x27; , CANTIDAD&#x29; SRV_CDD,&#xa;						   DECODE&#x28;ELEMENT_CLASS, &#x27;PAIS&#x27;   , CANTIDAD&#x29; SRV_CTY&#xa;					FROM &#x28;&#xa;						SELECT FECHA, ELEMENT_CLASS, COUNT&#x28;&#x2a;&#x29; CANTIDAD&#xa;						FROM LTE_NSN_SERVICE_NE_DAY&#xa;						WHERE FECHA BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;						GROUP BY FECHA, ELEMENT_CLASS&#xa;						&#x29;&#xa;					&#x29;&#xa;				GROUP BY FECHA								&#xa;			&#x29;&#xa;SELECT&#xa;	RFC.FECHA,&#xa;	AVL.AVL_LBS,&#xa;	AVL.AVL_ALM,&#xa;	AVL.AVL_MRC,&#xa;	AVL.AVL_CDD,&#xa;	AVL.AVL_CTY,&#xa;	PAQ.PAQ_LBS,&#xa;	PAQ.PAQ_ALM,&#xa;	PAQ.PAQ_MRC,&#xa;	PAQ.PAQ_CDD,&#xa;	PAQ.PAQ_CTY,&#xa;	SRV.SRV_LBS,&#xa;	SRV.SRV_ALM,&#xa;	SRV.SRV_MRC,&#xa;	SRV.SRV_CDD,&#xa;	SRV.SRV_CTY&#xa;FROM&#xa;RFC,&#xa;AVL,&#xa;PAQ,&#xa;SRV&#xa;WHERE&#xa;RFC.FECHA&#x3d;AVL.FECHA &#x28;&#x2b;&#x29;&#xa;AND RFC.FECHA&#x3d;PAQ.FECHA&#x28;&#x2b;&#x29;&#xa;AND RFC.FECHA&#x3d;SRV.FECHA&#x28;&#x2b;&#x29;&#xa;ORDER BY RFC.FECHA</sql>
