    <connection>conn_perdido</connection>
    <sql>WITH RFC as&#x28;&#xa;        SELECT FECHA&#xa;          FROM CALIDAD_STATUS_REFERENCES&#xa;         WHERE FECHA BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;           AND HORA &#x3d; &#x27;00&#x27;&#xa;       &#x29;,&#xa;       WCE as&#x28;&#xa;        SELECT PERIOD_START_TIME FECHA, COUNT&#x28;1&#x29; CANTIDAD&#xa;          FROM UMTSC_NSN_MACD_WCEL_DAY&#xa;         WHERE PERIOD_START_TIME BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;         GROUP BY PERIOD_START_TIME &#xa;       &#x29;,&#xa;      RNC as &#x28;&#xa;        SELECT PERIOD_START_TIME FECHA, COUNT&#x28;1&#x29; CANTIDAD&#xa;          FROM UMTSC_NSN_MACD_RNC_DAY&#xa;         WHERE PERIOD_START_TIME BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;         GROUP BY PERIOD_START_TIME &#xa;       &#x29;,&#xa;       ALM as&#x28;&#xa;        SELECT PERIOD_START_TIME FECHA, COUNT&#x28;1&#x29; CANTIDAD&#xa;          FROM UMTSC_NSN_MACD_ALM_DAY&#xa;         WHERE PERIOD_START_TIME BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;         GROUP BY PERIOD_START_TIME &#xa;       &#x29;,&#xa;       MKT as&#x28;&#xa;        SELECT PERIOD_START_TIME FECHA, COUNT&#x28;1&#x29; CANTIDAD&#xa;          FROM UMTSC_NSN_MACD_MKT_DAY&#xa;         WHERE PERIOD_START_TIME BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;         GROUP BY PERIOD_START_TIME &#xa;       &#x29;,&#xa;      CTY as &#x28;&#xa;        SELECT PERIOD_START_TIME FECHA, COUNT&#x28;1&#x29; CANTIDAD&#xa;          FROM UMTSC_NSN_MACD_PAIS_DAY&#xa;         WHERE PERIOD_START_TIME BETWEEN TRUNC&#x28;SYSDATE&#x29;-7&#xa;                                AND TRUNC&#x28;SYSDATE&#x29;-1&#xa;         GROUP BY PERIOD_START_TIME &#xa;       &#x29;&#xa;SELECT RFC.FECHA,&#xa;       WCE.CANTIDAD CNT_WCE,&#xa;       RNC.CANTIDAD CNT_RNC,&#xa;       ALM.CANTIDAD CNT_ALM,&#xa;       MKT.CANTIDAD CNT_MKT,&#xa;       CTY.CANTIDAD CNT_CTY&#xa;FROM&#xa;	RFC,&#xa;	WCE,&#xa;	RNC,&#xa;	ALM,&#xa;	MKT,&#xa;	CTY&#xa; WHERE RFC.FECHA &#x3d; WCE.FECHA &#x28;&#x2b;&#x29;&#xa;   AND RFC.FECHA &#x3d; RNC.FECHA &#x28;&#x2b;&#x29;&#xa;   AND RFC.FECHA &#x3d; ALM.FECHA &#x28;&#x2b;&#x29;&#xa;   AND RFC.FECHA &#x3d; MKT.FECHA &#x28;&#x2b;&#x29;&#xa;   AND RFC.FECHA &#x3d; CTY.FECHA &#x28;&#x2b;&#x29;&#xa; ORDER BY RFC.FECHA&#xa;</sql>
    <limit>0</limit>
